{
    "name": "rubiconinternational/passportr",
    "description": "Laravel Passport provides OAuth2 server support to Laravel. This also has relationships",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "firebase/php-jwt": "~3.0|~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~6.0",
        "illuminate/console": "~6.0",
        "illuminate/container": "~6.0",
        "illuminate/contracts": "~6.0",
        "illuminate/database": "~6.0",
        "illuminate/encryption": "~6.0",
        "illuminate/http": "~6.0",
        "illuminate/support": "~6.0",
        "league/oauth2-server": "~6.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
